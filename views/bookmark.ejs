<%- include("templates/header") %> 

<main>

    <section class="py-5 text-center">
      <div class="row py-lg-5">
        <div class="col-lg-6 col-md-8 mx-auto">
          <h1 class="display-4 fw-bold mb-5"> Bookmarks </h1>
          <p class="text-body-secondary"> <%= name %>'s bookmarks. <br>
            Click on the card to view the dog breed. <br>
            Click on the icon for shortcut un-bookmark and re-bookmark! </p>
        </div>
      </div>
    </section>
  
    <div class="album py-5">
        <div class="container">
          <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">

            <% let i=0; %>
            <% for(i=0; i < bookmarkedDogs.length; i++) { %>
            <%- include('templates/bookmarkCard', {bookmarkedDog: bookmarkedDogs[i]}) %>
            <% } %>
            
          </div>
        </div>
      </div>
  
</main>

<%- include("templates/footer") %> 